{
  "list.normalMappings": {
    "<C-j>": "do:exit",
    "<C-k>": "do:exit"
  },
  "highlight.colors.enable": true,
  "diagnostic.checkCurrentLine": true,
  "workspace.workspaceFolderCheckCwd": false,
  "workspace.bottomUpFiletypes": [
    "*"
  ],
  "diagnostic.level": "error",
  "python.formatting.provider": "ruff",
  "python.linting.ruffEnabled": true,
  "pyright.disableDiagnostics": false,
  "json.format.enable": true,
  "[lua]": {
    "inlayHint.enable": false
  },
  "[go][python][json][jsonc][rust][lua]": {
    "coc.preferences.formatOnSave": true
  },
  "go.tests": {
    "generateFlags": []
  },
  "[terraform]": {
    "coc.preferences.formatOnSave": false
  },
  "languageserver": {
    "sourcekit-lsp": {
      "filetypes": [
        "swift"
      ],
      "command": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/sourcekit-lsp",
    },
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": [
        "terraform"
      ]
    }
  },
  "go.goplsEnv": {
    "GO111MODULE": "off"
  },
  "go.goplsOptions": {
    "analyses": {
      "shadow": true
    },
    "expandWorkspaceToModule": false,
    "buildFlags": [
      "-tags=integration"
    ],
    "directoryFilters": [
      "-**/vendor"
    ]
  },
  "go.tags": {
    "transform": "camelcase"
  },
  "clangd.fallbackFlags": [
    "-std=gnu++20"
  ],
  "git.addGBlameToBufferVar": true,
  "colors.enable": true,
  "snippets.ultisnips.pythonPrompt": false
}
